            <script>

              var url = '';

              if(_sf.query.hi == 1)
                $('#sf_form').closest('.wrapper').hide();

              if (sf.isPrimitiveBrowser()) {
                $('.downloader-2').addClass('simple');
              }
              else
              {
                var linkClipboard = new Clipboard('.link-copy-btn');
                linkClipboard.on('success', function(e){
                  e.clearSelection();
                  jAlert(sf.lng.copyLinkSuccess);
                });
                linkClipboard.on('error',function(e){
                  jAlertErr(sf.lng.copyLinkFail);
                });

                $('body').
                  on('click', function(event){
                    var $e = $('.link-box.expand .drop-down-box'), e = $e[0];
                    if(e && event.target != e)
                    {
                      $('.list', $e).slideUp('fast', function(){
                        $e.closest('.link-box').removeClass('expand');
                      });
                    }
                  }).
                  on('keyup', function(event){
                    var $e = $('.link-box.expand');
                    if($e[0] && event.which == 27)
                    {
                      $('.list', $e).slideUp('fast', function(){
                        $e.closest('.link-box').removeClass('expand');
                      });
                    }
                  }).
                  on('click', '.def-btn-box .link-download', function(){
                    if (window.ga) {
                      var type = $(this).attr('data-type') || '-';
                      window.ga('send', 'event', 'result', 'download_default_format', type);
                    }
                  }).
                  on('click', '.result-box .drop-down-box .link-download', function(e){
                    if (window.ga) {
                      var type = $(this).attr('data-type') || '-';
                      window.ga('send', 'event', 'result', 'download_custom_format', type);
                    }
                  }).
                  on('click', '.link-box .def-btn-name', function(event){
                    event.preventDefault();
                    event.stopPropagation();
                    var $this = $(this), $e = $this.closest('.link-box');
                    if($e.hasClass('expand'))
                    {
                      $('.list', $e).slideUp('fast', function(){
                        $e.removeClass('expand');
                      });
                    }
                    else
                    {
                      $('.link-box.expand .list').slideUp('fast', function(){
                        $(this).closest('.link-box').removeClass('expand');
                      });

                      $('.list', $e).slideDown('fast', function(){
                        $e.addClass('expand');
                      });

                      sf.result.trackElementAction(this, 'list-show', true);
                    }
                  }).
                  on('click', '.link-box .more-links a', function(event){
                    event.preventDefault();
                    event.stopPropagation();
                    $(this).closest('.link-box').toggleClass('showall');
                    sf.result.trackElementAction(this, 'more-btn-click', true);
                  });

                  // Click + Alt simulation for Safari browser
                  if (!_sf.mobile && _sf.browser.name == 'Safari') {
                    $('body').on('click', '.result-box .link-download', function(e){
                      if (e.button || e.ctrlKey || e.altKey || e.shitfKey) {
                        return;
                      }

                      if ($(this).attr('data-copy')) {
                        return;
                      }

                      var me = null;

                      try {
                        if (typeof MouseEvent == 'function') {
                          me = new MouseEvent('click', {
                            bubbles: e.bubbles,
                            cancelable: e.cancelable,
                            view: e.view,
                            screenX: e.screenX,
                            screenY: e.screenY,
                            clientX: e.clientX,
                            clientY: e.clientY,
                            altKey: true,
                            metaKey: e.metaKey,
                            button: e.button,
                            relatedTarget: e.relatedTarget
                          });
                        }
                        else {
                          me = document.createEvent('MouseEvents');
                          me.initMouseEvent('click', e.bubbles, e.cancelable, window, 0,
                            e.screenX, e.screenY, e.clientX, e.clientY,
                            false, true, false, e.metaKey, e.button, e.relatedTarget);
                        }
                      }
                      catch (err) {
                        console.log(err);
                        return;
                      }

                      if (me) {
                        
                        e.preventDefault();
                        e.stopPropagation();
                        this.dispatchEvent(me);
                      }
                    });
                  }
              }

              $('body').
                on('click', '.link-box a[data-copy]', function(event){
                  event.preventDefault();
                  var url = $(this).data('copy');
                  $.fancybox({
                    content: $('#tplCopyBox').render({text: url}),
                    beforeShow: function(){
                      $('.fancybox-inner .btn').addClass('zcopy');
                    }
                  });
                }).
                on('click', '.click-box', function(event){
                  var href = $(this).data('href');
                  if(href)
                  {
                    event.preventDefault();
                    if (window.gaHandler) {
                      window.gaHandler.trackNode(this);
                    }
                    window.open(href);
                  }
                });

              $(document).on('sf_form:ready', function() {
                var prevValue = '',
                    FORM_CONST = sf.getFormConst(),
                    $form = $('#' + FORM_CONST.FORM_ID),
                    $input = $('#' + FORM_CONST.INPUT_ID),
                    $clearBtn = $form.find('.clear-btn');

                // в event объекте jquery нет clipboardData в FF > 22, поэтому подписываемся через нативный слушатель
                if (document.addEventListener) {
                  document.getElementById(FORM_CONST.INPUT_ID).addEventListener('paste', sf.onPasteUrl);
                }
                $input.on('focus keyup click change input', function() {
                  var currentValue = $input.val();

                  if(currentValue) {
                    if(currentValue != prevValue) {
                      var valid = sf.validateUrl(currentValue);

                      $form.removeClass('result');

                      if(valid) {
                        $form.removeClass('invalid').addClass('ready');
                      } else {
                        $form.removeClass('ready').addClass('invalid');
                      }

                      prevValue = currentValue;
                    }

                    $clearBtn.show();

                  } else {
                    prevValue = '';
                    $form.removeClass('ready invalid');
                    $clearBtn.fadeOut('fast');
                  }
                });

                $clearBtn.on('click', function(event){
                  event.preventDefault();

                  prevValue = '';

                  $input.val('');
                  $form.removeClass('ready invalid');
                  $clearBtn.fadeOut('fast');
                  $input.focus();
                });

                if($(document).width() <= 680) {
                  $input.attr('placeholder', "Enter the URL");
                }

              });

              $(function(){
                function replaceLanguageSelectorLink(a, url)
                {
                  var href = a.href;
                  if(!href)
                    return;

                  href = href.replace(/^(https?:\/\/[\w\-\.]+\/)https?\:?\/\/?.+/i, '$1');
                  href = href.replace(/^(https?:\/\/[\w\-\.]+\/)\d+\-.+/i, '$1');

                  url = 'url=' + encodeURIComponent(url);

                  if(href.search(/(\?|#|&)url=[^\s\"&]+/i) == -1)
                  {
                    if(href.search(/#$/) != -1)
                      a.href = href + url;
                    else if(href.search(/#.+/) != -1)
                      a.href = href + '&' + url;
                    else
                      a.href = href + '#' + url;
                  }
                }

                var query = _sf.query;
                if(!query || !query.url)
                {
                  if(location.hash)
                  {
                    var hash = location.hash.replace(/(^|#|&)url=(https?:?\/\/?.+)$/i, function(s, p1, p2){
                      if(!p2)
                        return s;

                      return p1 + 'url=' + escape(p2);
                    });

                    query = sf.parseQuery(hash);
                  }
                }

                if(query && query.url)
                {
                  url = decodeURIComponent(query['url']);
                  url = url.replace(/^(https?):?\/\/?/i, '$1://');

                  if(url.search(/^https?:\/\//i) == -1)
                  {
                    url = 'http://' + url;
                  }
                }
                else
                {
                  var t = location.href.match(/^https?:\/\/[\w\-\.]+\/(https?\:?\/\/?[^\s\"]+)/i);
                  if(t && t.length > 1)
                  {
                    url = t[1].replace(/^(https?):?\/\/?/i, '$1://');
                    var domain = url.match(/^https?:\/\/([^\/+])/i);
                    if(domain && domain.length > 1)
                    {
                      domain = domain[1];
                      domain = domain.replace(/^[^@]+@/i, '').replace(/:\d+$/, '');
                      if(domain.indexOf('%') > -1)
                        url = decodeURIComponent(url);
                    }
                  }
                }

                if(url)
                {
                  url = url.replace(/(\?|#|&)_openstat=[^&#]*&/g, '$1');
                  url = url.replace(/[\?&#]_openstat=[^&#]*($|#)/, '$1');
                  url = url.replace(/(\?|#|&)(?:utm_[a-z]+=[^&#]*&?)+/ig, '$1').replace(/[\?&]#/, '#').replace(/[\?#&]+$/, '');
                  $('.lang :not(.active) a').each(function(){replaceLanguageSelectorLink(this, url);});

                  setTimeout(function(){
                    if (!$('body').hasClass('no-autosubmit')) {
                      $('input[name="sf_url"]').val(url);
                      sf.autoSubmitForm('sf_submit');
                    }
                  }, 1000);
                }
              });
            //-->
            </script>
