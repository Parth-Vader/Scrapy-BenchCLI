    <script type="text/javascript">(function ($) {

    /* 焦点图切换 */
    //Tab切换与焦点图轮播的结合,有点坑
    // 2017-03-14 轮播改版 沿用原有基础 发现确实有点坑
    // 风格过于imperative 依赖state
    // 待IE8退出历史舞台 希望后人能够淘汰jquery
    var focusA, focusB, focusC, tabView, curView = 0;

    var timerA, timerB, timerC, director;

    function clearTimer() {
        var timers = Array.prototype.slice.call(arguments);
        var len = timers.length;
        while (len > -1) {
            typeof timers[len - 1] === "number" && clearTimeout(timers[len - 1]);
            len--;
        }
    }

    function genFocus($dom, opt) {
        var conf = $dom.data('slide') || {};
        $.extend(conf, opt);
        var api = $dom.switchable(conf);
        $.extend($dom, {
            "switchable": api
        });
        return api;
    }
    focusA = genFocus($('.j-focus-a'), {
        'onSwitch': function (e) {
            if (e.target.index == (e.target.length - 1)) {
                e.target.pause();
                timerA = setTimeout(function () {
                    tabView.switchTo(1);
                }, e.target.config.interval);
            }
        }
    });
    focusB = genFocus($('.j-focus-b'), {
        'onSwitch': function (e) {
            if (e.target.index == (e.target.length - 1)) {
                e.target.pause();
                timerB = setTimeout(function () {
                    tabView.switchTo(2);
                }, e.target.config.interval);
            }
        }
    });
    focusC = genFocus($('.j-focus-c'), {
        'onSwitch': function (e) {
            if (e.target.index == (e.target.length - 1)) {
                e.target.pause();
                timerC = setTimeout(function () {
                    tabView.switchTo(0);
                }, e.target.config.interval);
            }
        }
    });
    focusB.pause();
    focusC.pause();

    tabView = genFocus($('.j-tabview'), {
        'onSwitch': function (e) {
            curView = e.target.index;
            clearTimer(timerA, timerB, timerC);
            if (e.target.index == 0) {
                if (director == 'prev') {
                    focusA.switchTo(focusA.length - 1);
                    director = 'next';
                } else {
                    focusA.switchTo(0);
                }
                focusB.switchTo(0).pause();
                focusC.switchTo(0).pause();
                // focusA.switchTo(0);
                focusA.play();
            } else if (e.target.index == 1) {
                if (director == 'prev') {
                    focusB.switchTo(focusB.length - 1);
                    director = 'next';
                } else {
                    focusB.switchTo(0);
                }
                focusA.switchTo(0).pause();
                focusC.switchTo(0).pause();
                focusB.play();
            } else if (e.target.index == 2) {
                if (director == 'prev') {
                    focusC.switchTo(focusC.length - 1);
                    director = 'next';
                } else {
                    focusC.switchTo(0);
                }
                focusA.switchTo(0).pause();
                focusB.switchTo(0).pause();
                focusC.play();
            }

        }
    });

    $('.j-tabview').delegate('.btn-next', 'click', function (e) {
        e.preventDefault();
        var curSlide, nextView;

        director = 'next';

        clearTimer(timerA, timerB, timerC)

        if (curView == 0) {
            curSlide = focusA;
            nextView = 1;
        } else if (curView == 1) {
            curSlide = focusB;
            nextView = 2;
        } else if (curView == 2) {
            curSlide = focusC;
            nextView = 0;
        }

        if (curSlide._nextIndex == 0) {
            curSlide.pause();
            tabView.switchTo(nextView);
            return false;
        }
        curSlide.switchTo(curSlide._nextIndex);
        curSlide.pause();
    })
    $('.j-tabview').delegate('.btn-prev', 'click', function (e) {
        e.preventDefault();
        var curSlide, nextView;

        director = 'prev';

        clearTimer(timerA, timerB, timerC);

        if (curView == 0) {
            curSlide = focusA;
            nextView = 2;
        } else if (curView == 1) {
            curSlide = focusB;
            nextView = 0;
        } else if (curView == 2) {
            curSlide = focusC;
            nextView = 1;
        }

        var curIndex = curSlide.index;
        if (curSlide.index == 0) {
            curSlide.pause();
            tabView.switchTo(nextView);
            return false;
        }
        curSlide.switchTo(curSlide.index - 1);
        curSlide.pause();
    })
