			uiContainerElem:null,
			posAdjust:{},
			autoPosition: true // 是否自动定位，用于fixed需要
		};
		extend(true,this._config, config);
		this.container = null,
		this.searchForm = null,
		this.isWatching = false,

		this._groupHandlerNames = [],
		this._groupHandler = {},
		this._groupTotal = {},
		this._focusedItemIndex = {},//以-1为标识 next步骤中要特别处理 (TOTAL-1)+1!==-1其中TOTAL>0
		this._focusedGroupIndex = -1,
		//与INPUT有关
		this.textInput = null,
		this.query = "";
		this.lastInputVal = "";
