    <script type="text/javascript">/*
 * 二维码显示隐藏
 * 一些手机软件的下载链接，鼠标划过显示二维码
 * 有data-qr属性，值为二维码图片url
 */
 ;(function ($) {

 	var app = function () {
 		var IMG_TMPL = '<img src="{$url}" id="{$id}" />'.tmpl(),
 			MARGIN = '10',
 			WIDTH = '120',
 			HEIGHT = '120';


 		var id, style = {
			'position': 'absolute',
			'width': WIDTH,
			'height': HEIGHT,
            'z-index': '100'
		};

		var getOffset = function ($dom) {
			var offset = $dom.offset();
			var width = $dom.width();

			var newLeft = offset.left + width/2 - WIDTH/2;
			var newTop = offset.top - MARGIN - HEIGHT;

			return {
				left: newLeft,
				top: newTop
			}
		}

		var bindEvent = function () {
			$('a[data-qr]').on('mouseenter', function () {
				var $this = $(this),
					qr = $this.data('qr');

				id = "q" + (+new Date());

				if(qr.trim()){
					$(IMG_TMPL({
						"id": id,
						"url": qr
					})).appendTo('body').css($.extend(style, getOffset($this)));
				}
			}).on('mouseleave', function (e) {
				$('#' + id).remove();
			})
		}

 		

 		return {
 			init: function () {
 				bindEvent();
 			}
 		}
 	}();

 	app.init();


 })(jQuery);</script>
