// ------------------------------
// SSO登录begin

// 获得指定名称的cookie值
function getCookie(c_name) {
    if (document.cookie.length > 0) {
        c_start = document.cookie.indexOf(c_name + "=");
        if (c_start != -1) {
            c_start = c_start + c_name.length + 1;
            c_end = document.cookie.indexOf(";", c_start);
            if (c_end == -1){c_end = document.cookie.length;}
            return unescape(document.cookie.substring(c_start, c_end));
        }
    }
    return "";
}

// 判断是否已登录
function if_login() {
    var cookie_name = decodeURI(getCookie('Example_auth_username'));
    if ($('#loginbar_new').length > 0) {
        var if_loginbar_new = 1;
    }
    if (if_loginbar_new == 1) {
        if (cookie_name.length > 0 && cookie_name != "null") {
            $('#loginbar_new').html("<div id='login'>" + cookie_name + "&nbsp;&nbsp;您好!&nbsp;&nbsp;<a style='color:#FDCF48;' href='http://blog.gmw.cn/' target='_blank'>进入光明博客</a>&nbsp;&nbsp;<a href='#' onclick=\"login_in();\" target='_self'>退出</a></div>");
        } else {
            $('#loginbar_new').html('<input type="button" style="background:url(http://img.gmw.cn/pic/login_gmw.jpg) no-repeat;width: 60px;height:18px;border:0;margin: 5px 0px;" onclick="gotoLogin(\'http://www.gmw.cn\')">');
        }
    }
}

function gotoLogin(backurl){
	window.location.href = 'http://home.gmw.cn/index.php?callback='+encodeURIComponent(backurl);
}

//登陆通信
function login_in(y) {
    $("#login").hide();
    var loginbar_new_html = $('#loginbar_new').html();
    $('#loginbar_new').html("&nbsp;<img border=0 height=20 src=\"http://home.gmw.cn/wait.gif\" />&nbsp;加载中..." + loginbar_new_html);
    var iframe = document.createElement("iframe");
    iframe.id = "login_id";
    iframe.style.display = "none";
    var login_url = "http://home.gmw.cn/www_login.php?do=logout";
    iframe.src = login_url;
    if (iframe.attachEvent) {
        iframe.attachEvent("onload", function() {
            location.reload();
            //if_login();
            document.body.removeChild(iframe);
        });
    } else {
        iframe.onload = function() {
            location.reload();
            //if_login();
            document.body.removeChild(iframe);
        };
    }
    document.body.appendChild(iframe);
}
var cookie_name = decodeURIComponent(getCookie('Example_auth_username'));
document.form2.searchText.size = '16'; //控制头部 搜索 文本框的长度


